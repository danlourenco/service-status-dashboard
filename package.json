{
  "name": "service-status-dashboard",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "release": "npm run release:patch",
    "release:patch": "npm version patch && npm run changelog && git add CHANGELOG.md && git commit --amend --no-edit && git push --follow-tags",
    "release:minor": "npm version minor && npm run changelog && git add CHANGELOG.md && git commit --amend --no-edit && git push --follow-tags",
    "release:major": "npm version major && npm run changelog && git add CHANGELOG.md && git commit --amend --no-edit && git push --follow-tags",
    "changelog": "changelogen --release"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.5",
    "c12": "^3.0.4",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "httpxy": "^0.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
